@model LabTask1.Models.Student
@{
    ViewBag.Title = "Edit";
}

<h2>Edit</h2>
<form action="/Student/Edit" method="post">
    <input type="hidden" name="Id" class="form-control" value="@Model.Id" />
    <input type="text" name="Name" class="form-control" value="@Model.Name" />
    <input type="date" name="Dob" class="form-control" value="@Model.Dob" />
    <input type="text" name="Credit" class="form-control" value="@Model.Credit" />
    <input type="text" name="CGPA" class="form-control" value="@Model.CGPA" />
    <input type="text" name="Dept" class="form-control" value="@Model.Dept" />
    <input type="submit" class="btn btn-block" value="Edit" />
</form>

